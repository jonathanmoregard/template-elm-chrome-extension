{
  "name": "template-elm-chrome-extension",
  "version": "1.0.0",
  "description": "A template repository for building a chrome extension with webpack and elm.",
  "main": "TemplateElmChromeExtension/app.html",
  "scripts": {
    "debug": "webpack --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js --watch",
    "live": "webpack-dev-server --config webpack.live.js --open --hot",
    "build": "webpack --config webpack.prod.js",
    "start": "npm run live",
    "restart": "rm -rf elm-stuff && npm start",
    "test": "elm-test",
    "prod": "npm run build && zip -r ./TemplateElmChromeExtension.zip ./TemplateElmChromeExtension"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/tlentz/template-elm-chrome-extension.git"
  },
  "bugs": {
    "url": "https://github.com/tlentz/template-elm-chrome-extension/issues"
  },
  "homepage": "https://github.com/tlentz/template-elm-chrome-extension#readme",
  "keywords": [
    "template",
    "chrome-extension",
    "elm",
    "webpack"
  ],
  "author": "Tyler Lentz",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chrome-extension-async": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-transform-classes": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "axios": "^1.6.8",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "elm": "^0.19.1-3",
    "elm-hot-webpack-loader": "^1.1.6",
    "elm-minify": "^1.5.3",
    "elm-test": "^0.19.1-revision2",
    "elm-webpack-loader": "^7.0.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "fibers": "^4.0.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "^3.5.1",
    "less": "^3.13.1",
    "node-sass": "^9.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^14.1.1",
    "style-loader": "0.23.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-ext-reloader": "^1.1.12",
    "webpack-merge": "4.2.1"
  }
}